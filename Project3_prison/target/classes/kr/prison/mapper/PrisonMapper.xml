<mapper namespace="kr.prison.mapper.PrisonMapper">

	<select id="AnomalyDetectionHistoryAjax" resultType="kr.prison.domain.AnomalyDetectionHistory"> <!-- DAO는 맵핑 용도로 변경된다 고로 DAO에서 작업할 필요가 없다 -->
