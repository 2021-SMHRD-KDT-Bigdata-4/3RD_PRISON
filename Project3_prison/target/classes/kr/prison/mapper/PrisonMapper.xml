		select * from CORRECTIVE_HISTORY
	</select>
	
	<select id="DrugManagementVOAjax" resultType="kr.prison.domain.DrugManagementVO"> <!-- DAO는 맵핑 용도로 변경된다 고로 DAO에서 작업할 필요가 없다 -->
